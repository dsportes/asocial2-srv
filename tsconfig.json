{
  "compilerOptions": {
    "module": "NodeNext",
    "moduleResolution": "nodenext",
    "target": "ES6",
    "strict": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  },

  "include": ["src/**/*", "src-fw/**/*", "src-dbst/**/*", "src-fs/**/*", "src-sq/**/*", "src-sl/index.ts",],
  "exclude": ["**/*.spec.ts"]
}
